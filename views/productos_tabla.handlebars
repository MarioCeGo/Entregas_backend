<h3>Productos registrados</h3>
<a href="/registrar"><button>Registrar productos</button></a>
<table>
    <tr>
        <th>Nombre</th>
        <th>Precio</th>
        <th>Foto</th>
    </tr>
    {{#if datos.length}}
    {{#each datos}}
    <tr>
        <td>{{this.title}}</td>
        <td>{{this.price}}</td>
        <td><img src={{this.thumbnail}} alt="" height="50px" width="50px"></td>
    </tr>
    {{/each}}
    {{else}}
    <p>No hay productos</p>
    {{/if}}
</table>
